URxvt.font:	xft:Envy Code R:size=9:antialias=true:hinting=true

urxvt*urlLauncher:      chromium
urxvt*geometry: 	90x22
urxvt*externalBorder:   0
urxvt*internalBorder:   0
urxvt*background:       rgba:0000/0000/0000/dddd
urxvt*depth:		32
urxvt*foreground:       #eeeeee
urxvt*fading:   	15
urxvt*tintColor:        white
urxvt*shading:  	30
urxvt*inheritPixmap:    True
urxvt*scrollBar:        False
urxvt*saveLines:        10000
urxvt*cursorBlink:      true
!urxvt*font:     xft:DejaVu Sans Mono:pixelsize=11:antialias=true:hinting=true
urxvt*secondaryScroll:  true    # Enable Shift-PageUp/Down in screen
urxvt*pastableTabs:     true
urxvt*title:            Terminal
urxvt*perl-ext-common:  default,matcher
urxvt.matcher.button:   1

! Xft font settings
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

! Colors
!!Black (not tango) + DarkGrey
*color0:         rgb:00/00/00
*color8:         rgb:55/57/53
!!red
*color1:         rgb:85/0d/0d
*color9:         rgb:98/2b/2b
!!green
*color2:         rgb:88/be/32
*color10:        rgb:af/f7/3e
!!yellow (orange)
*color3:         rgb:ff/7e/00
*color11:        rgb:fa/d4/3d
!!blue
*color4:         rgb:2e/4c/99
*color12:        rgb:33/6e/c0
!!magenta
*color5:         rgb:70/6c/9a
*color13:        rgb:82/6a/b1
!!cyan
*color6:         rgb:00/90/ff
*color14:        rgb:80/b0/dd
!!white
*color7:         rgb:aa/aa/aa
*color15:        rgb:ff/ff/ff
